---
import BaseLayout from '../layouts/BaseLayout.astro';
import SaraBicudoMobile from "/assets/svg/sara_bicudo_thin.svg?raw"
import SaraBicudoDesktop from "/assets/svg/sara_bicudo_wide.svg?raw"
const pageTitle = "Home Page";
const hidePageTitle = true;
---

<BaseLayout pageTitle={pageTitle} hidePageTitle={hidePageTitle}>
  <Fragment slot="head">
    <style>
      h2 {
        text-align: left;
        margin: 0 0 8px 0;
      }
  
      h3 {
        text-align: left;
        margin: 40px 0 40px 0;
      }
  
      p {
        margin-top: 40px;
        text-align: center;
      }
      
      .illustration {
        display: block;
        max-width: 100%;
        width: 60%;
        height: auto;
        margin: 0 auto;
      }
  
      .intro {
        margin: 0 auto;
        width: 80%
      }
  
      .sub-header {
        padding: 120px 0 40px 0;
      }
  
      .name p {
        display: none;
      }
  
      @media (max-width: 870px) {
        .hide-desktop {
            display: none;
        }
        .hide-mobile {
          display: block;
          margin: 0;
          margin-top: 20px;
          margin-bottom: 20px;
          height: auto;
        }
      }
    
      @media (min-width: 871px) {
        .hide-mobile {
          display: none;
        }
        .hide-desktop {
          display: block;
          margin-top: 40px;
          margin-bottom: 40px;
          height: auto;
          margin: 0 auto;
        }
  
        .illustration {
          top: 40px;
          margin-bottom: 40px;
        }
        .intro h2,
        .intro h3 {
          text-align: center;
        }
      }
  
        #rotating-ellipse {
            animation: rotate 10s linear infinite;
            transform-origin: 142px 107px; /* Coordinates for the center of the ellipse */
        }
  
        #sparkling-star {
            animation: sparkle 2s ease-in-out infinite;
        }
  
        @keyframes rotate {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
        }
  
        @keyframes sparkle {
            0% { opacity: 1; }
            20% { opacity: 0.8; }
            40% { opacity: 0.5; }
            50% { opacity: 0; }
            60% { opacity: 0.5; }
            80% { opacity: 0.8; }
            100% { opacity: 1; }
        }
    </style>
  </Fragment>
  <main class="sub-header">
    <section class="intro">
      <h2>Hello, I'm</h2>
      <figure class="hide-desktop" aria-labelledby="sara bicudo text black, grey and lilac text illustration">
          <Fragment set:html={SaraBicudoDesktop}/>
      </figure>
      <figure class="hide-mobile" aria-labelledby="sara bicudo text black, grey and lilac text illustration">
          <Fragment set:html={SaraBicudoMobile}/>
      </figure>
      <h3 style="font-weight:300">A Digital Designer based in Lisbon</h3>
    </section>
    <figure class="illustration" aria-labelledby="lilac, green, yellow and pink shapes all mixed together">
